{
    "name": "Selander/DemoApp",
    "autoload": {
        "psr-4": {
            "Selander\\DemoApp\\": "src/",
            "Selander\\Framework\\": "../framework/src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "../framework"
        }
    ],
    "require": {
        "Selander/Framework": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Selander\\DemoApp\\Tests\\": "tests/"
        }
    }
}
